{
  "name": "waifu_valt",
  "version": "1.0.0",
  "description": "Temp file hosting for nerds",
  "type": "module",
  "scripts": {
    "tsc": "tsc --project tsconfig.compile.json",
    "build": "npm run barrels && node ./buildScripts/cleanDist.mjs && npx swc src -d dist --config-file .swcrc --copy-files && node ./buildScripts/moveFiles.mjs",
    "barrels": "barrelsby --config .barrelsby.json && node ./buildScripts/fixbarrels.cjs",
    "start_js": "node dist/index.js",
    "start": "node --loader ts-node/esm src/index.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon dist/index.js",
    "update_deps": "npx npm-check-updates",
    "typeorm": "typeorm-ts-node-esm",
    "blankmigration": "npx typeorm-ts-node-esm migration:create",
    "generatemigration": "npx typeorm-ts-node-esm migration:generate -d ./src/db/DataSource.ts",
    "runmigration": "npx typeorm-ts-node-esm migration:run -d ./src/db/DataSource.ts",
    "revertmigration": "npx typeorm-ts-node-esm migration:revert -d ./src/db/DataSource.ts"
  },
  "dependencies": {
    "@tsed/ajv": "^7.61.2",
    "@tsed/common": "^7.61.2",
    "@tsed/core": "^7.61.2",
    "@tsed/di": "^7.61.2",
