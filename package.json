    "@tsed/exceptions": "^7.61.2",
    "@tsed/json-mapper": "^7.61.2",
    "@tsed/logger": "^6.6.3",
    "@tsed/logger-file": "^6.6.3",
    "@tsed/passport": "^7.61.2",
    "@tsed/platform-cache": "^7.61.2",
    "@tsed/platform-exceptions": "^7.61.2",
    "@tsed/platform-express": "^7.61.2",
    "@tsed/platform-log-middleware": "^7.61.2",
    "@tsed/platform-middlewares": "^7.61.2",
    "@tsed/platform-params": "^7.61.2",
    "@tsed/platform-response-filter": "^7.61.2",
    "@tsed/platform-views": "^7.61.2",
    "@tsed/schema": "^7.61.2",
    "@tsed/swagger": "^7.61.2",
    "adm-zip": "^0.5.10",
    "ajv": "^8.12.0",
    "argon2": "^0.31.2",
    "better-sqlite3": "^9.4.2",
    "body-parser": "^1.20.2",
